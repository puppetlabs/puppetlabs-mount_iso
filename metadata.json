{
  "name": "puppetlabs/mount_iso",
  "version": "1.0.0",
  "author": "Puppet Labs",
  "summary": "Mount ISO images and ensure the drive letter they are mounted to",
  "license": "Apache-2.0",
  "source": "https://github.com/puppetlabs/puppetlabs-mount_iso",
  "project_page": "https://github.com/puppetlabs/puppetlabs-mount_iso",
  "issues_url": "https://tickets.puppetlabs.com/browse/MODULES",
  "tags": [
    "mount",
    "image",
    "iso",
    "powershell"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "Server 2016",
        "8",
        "10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-powershell",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    }
  ]
}

