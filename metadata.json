    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": "4.x"
    },
    {
      "name": "puppetlabs/powershell",
      "version_requirement": "1.x"
    }
  ]
}

